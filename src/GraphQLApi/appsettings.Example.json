{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JwtAuthSettings": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "xxxxx.onmicrosoft.com", // Domain of the Active Directory tenant
    "TenantId": "xxxx-xxxx-xxxx-xxxx", // Tenant ID of Active Directory
    "ClientId": "xxxx-xxxx-xxxx-xxxx", // Application ID of the App Registration (backend app)
    "SensitiveDataClientWhiteList": "" // Application IDs of whitelisted clients
  },
  "GraphQL": {
    "EnableMetrics": "false",
    "ExposeExceptions": "true",
    "MaxQueryDepth": "15"
  },
  "ConnectionStrings": {
    "Database": "xxxxxxxx"
  }
}
